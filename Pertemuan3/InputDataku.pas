
Program dataku;

Uses crt;

Var 
  npm_mahasiswa : string;
  nama_mahasiswa : string ;
  umur_mahasiswa : byte;
Begin
  clrscr;
  writeln('Masukkan Nama');
  readln(nama_mahasiswa);
  writeln('Masukkan NPM');
  readln(npm_mahasiswa);
  writeln('Masukkan Umur');
  readln(umur_mahasiswa);
  writeln(nama_mahasiswa);
  writeln(umur_mahasiswa);
  writeln(npm_mahasiswa);
  readln;
End.
